<h1>Listing store_statuses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @store_statuses.each do |store_status| %>
      <tr>
        <td><%= store_status.name %></td>
        <td><%= store_status.description %></td>
        <td><%= link_to 'Show', store_status %></td>
        <td><%= link_to 'Edit', edit_store_status_path(store_status) %></td>
        <td><%= link_to 'Destroy', store_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store status', new_store_status_path %>
